              ]
            },
            "removePostfixes": {
              "#": "no-translations",
              "nl": [
                "baan",
                "boulevard",
                "dreef",
                "heirbaan",
                "kaai",
                "kerk",
                "laan",
                "lei",
                "pad",
                "park",
                "plein",
                "ring",
                "steenweg",
                "straat",
                "weg",
                "wegel"
              ],
              "fr": [
                "parque"
              ],
              "de": [
                "straße",
                "strasse",
                "platz",
                "gasse",
                "graben",
                "grundschule",
                "gymnasium",
                "platz",
                "rain",
                "schule",
                "(-)?straße",
                "(-)?strasse",
                "weg"
              ],
              "en": [
                "avenue",
                "boulevard",
                "circle",
                "church",
                "drive",
                "expressway",
                "freeway",
                "highway",
                "lane",
                "parkway",
