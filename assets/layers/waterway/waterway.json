    {
      "color": "blue",
      "width": {
        "mappings": [
          {
            "if": "waterway=canal",
            "then": "8"
          },
          {
            "if": "waterway=river",
            "then": "8"
          },
          {
            "if": "waterway=stream",
            "then": "6"
          }
        ]
      }
    }
  ],
  "tagRenderings": [
    "images",
    "wikipedia",
    {
